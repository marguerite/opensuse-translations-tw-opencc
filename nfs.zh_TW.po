#. Command line help text for the nfs module
#: src/clients/nfs.rb:42
msgid "Configuration of NFS client"
msgstr "網路檔案系統客戶端配置"

#. command line action help
#: src/clients/nfs.rb:56
msgid "List configured NFS mounts"
msgstr "列出已配置的網路檔案系統載具"

#. command line action help
#: src/clients/nfs.rb:63
msgid "Add an NFS mount"
msgstr "添加網路檔案系統載具"

#. command line action help
#: src/clients/nfs.rb:68
msgid "Edit an NFS mount"
msgstr "編輯網路檔案系統載具"

#. command line action help
#: src/clients/nfs.rb:73
msgid "Delete an NFS mount"
msgstr "刪除網路檔案系統載具"

#. host:path
#. command line option help
#. fstab(5): fs_spec
#: src/clients/nfs.rb:83
msgid "Remote file system (in the form 'host:path')"
msgstr "遠端檔案系統 (採用 'host:path' 形式)"

#. path
#. command line option help
#. fstab(5): fs_file
#: src/clients/nfs.rb:92
msgid "Local mount point"
msgstr "本地裝載點"

#. a list?
#. command line option help
#. fstab(5): fs_mntops
#: src/clients/nfs.rb:102
msgid "Mount options"
msgstr "裝載選項"

#. nfs or nfs4
#. command line option help
#. fstab(5): fs_type
#: src/clients/nfs.rb:111
msgid "File system ID, supported nfs and nfs4. Default value is nfs."
msgstr ""
"檔案系統 ID，支援網路檔案系統與第四版網路檔案系統。預設值是網路檔案系統。"

