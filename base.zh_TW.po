#: library/commandline/src/modules/CommandLine.rb:749
msgid "This is a YaST module."
msgstr "這是一個 YaST 模組。"

#. translators: short help title for command line
#: library/commandline/src/modules/CommandLine.rb:754
msgid "Basic Syntax:"
msgstr "基本語法："

